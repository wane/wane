    "brotli-size": "0.0.2",
    "browser-sync": "^2.24.6",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "chokidar": "^2.0.4",
    "code-block-writer": "^6.10.1",
    "commander": "^2.17.1",
    "common-tags": "^1.8.0",
    "css-tree": "^1.0.0-alpha.29",
    "del": "^3.0.0",
    "format-number": "^3.0.0",
    "fs-extra": "^6.0.1",
    "gzip-size": "^4.1.0",
    "iterare": "^0.0.8",
    "joi": "^13.6.0",
    "lodash": "^4.17.10",
    "magic-string": "^0.24.0",
    "node-sass": "^4.9.2",
    "ora": "^3.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map": "^0.7.2",
    "table": "^4.0.3",
